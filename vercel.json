{
  "version": 2,
  "name": "phoenixops",
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://stackintechnologiesaollc.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
